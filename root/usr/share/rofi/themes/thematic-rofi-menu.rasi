@import "~/.config/rofi/color.rasi"

window {
padding: 20px;
width: 680px;
border-radius: 40px 0px 80px 0px;
}

mainbox {
children: [listview];
}


listview, {
border: 0px;
margin: 10px;
scrollbar: false;
lines:   1;
columns: 5;
}

element {
children: [ element-icon ];
orientation: vertical;
border-radius: 20px 0px 40px 0px;
border: 1px solid;
}

element selected {
border: 5px solid;
}

element-icon {
size: 480px;
}
